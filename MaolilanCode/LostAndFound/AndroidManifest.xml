<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.lostandfound"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".LostAndFoundActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".Menu"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.lostandfound.MENU" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".Found"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.lostandfound.Found" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
         <activity android:name=".Lost"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.lostandfound.Lost" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name=".HttpParseJSON"
                  android:label="@string/app_name">
        <intent-filter>
                <action android:name="com.lostandfound.HttpParseJSON" />
                <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        
        <activity android:name=".Submit"
                  android:label="@string/app_name">
        <intent-filter>
                <action android:name="com.lostandfound.Submit" />
                <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>

    </application>
</manifest>